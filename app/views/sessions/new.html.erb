<h1>ログイン</h1>
<div>
  <%= form_with url: login_path do |form| %>
    <div>
      <%= flash[:notice] %>
    </div>
    <div>
      <%= form.label :identification, "ログインID" %>
      <%= form.text_field :identification %>
    </div>
    <div>
      <%= form.label :password, "パスワード" %>
      <%= form.password_field :password %>
    </div>
    <div>
      <%= form.button "ログインする" %>
    </div>
  <% end %>
</div>
